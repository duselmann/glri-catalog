              <stringProp name="parameters"></stringProp>
              <stringProp name="script">var respBody = prev.getResponseDataAsString();
var item = JSON.parse(respBody);


//&apos;return&apos; values
var updatedValue = &quot;&quot;;
var hasUpdatedValue = false;

var LABEL = &quot;Label&quot;;
var SCHEME = &quot;https://www.sciencebase.gov/vocab/category/Great%20Lakes%20Restoration%20Initiative/GLRIWaterFeature&quot;;
var ALL_NAME = &quot;Lake:All&quot;;

var LAKE_LIST = [
	&quot;Lake:Lake Michigan&quot;,
	&quot;Lake:Lake Erie&quot;,
	&quot;Lake:Lake Huron&quot;,
	&quot;Lake:Lake Superior&quot;,
	&quot;Lake:Lake Ontario&quot;,
	&quot;Lake:Lake St. Clair&quot;
];


var tags = item.tags;
var newTags = [];	//empty array to put preserved and new tags into

if (tags) {

	//Remove tags w/ the old scheme and the new scheme
	for (i = 0; i &lt; tags.length; i++) {

		//Remove old tags from the list of tags
		if (tags[i].scheme == SCHEME &amp;&amp; tags[i].name == ALL_NAME) {
			//rm by not copying over
			hasUpdatedValue = true;
		} else if (tags[i].scheme == SCHEME) {
			//rm by not copying over
			//hasUpdatedValue = true;	//only set true if the &apos;All&apos; value is found, but prepare tags for edit by removing all WaterFeature entries regardless
		} else {
			//Not one of the tags we are looking for, so just copy it over from old to new
			newTags.push(tags[i]);
		}
	}

	if (hasUpdatedValue) {

		LAKE_LIST.forEach(function(entry) {
			var newTag = new Object();
			newTag.type = LABEL;
			newTag.scheme = SCHEME;
			newTag.name = entry;
			
			newTags.push(newTag);
		});


		updatedValue = JSON.stringify(newTags);

	}
}

vars.put(&quot;updatedValue&quot;, updatedValue);
vars.put(&quot;hasUpdatedValue&quot;, hasUpdatedValue);
</stringProp>
